[Unit]
Description=Data Manager Web
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/var/gaussian/data-manager
ExecStart=/var/gaussian/data-manager/venv/bin/python /var/gaussian/data-manager/run.py web --host 0.0.0.0 --port 5050
Restart=always
RestartSec=5
EnvironmentFile=/var/gaussian/data-manager/.env

[Install]
WantedBy=multi-user.target
