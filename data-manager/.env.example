# Data Manager Environment Configuration
# Copy this file to .env and fill in your API keys

# Tradier API (for fetching options data)
TRADIER_DATA_API_TOKEN=your_tradier_data_api_token
TRADIER_ACCESS_TOKEN=your_tradier_access_token

# Polygon API (optional - for historical data)
POLYGON_API_KEY=your_polygon_api_key

# Database path (optional - defaults to data/data_manager.db)
# DM_DB_PATH=/path/to/database.db

# Config path (optional - defaults to config.json)
# DM_CONFIG_PATH=/path/to/config.json
